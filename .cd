#!/bin/bash
title "Orlando Babysitters"
debug "aka. Passive Income"
help .cd | indent

function model() {
  plantuml state.puml
}

function listen() {
  watch -n 1 plantuml *.puml
}

function save() {
  if [ -z "$1" ]; then
    err "Usage: save \"did something neat>\""
    return; 
  fi
  git add .
  git commit -m "\"$@\""
  git push
}
